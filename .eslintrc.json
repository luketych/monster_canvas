{
    "root": true,
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": [
        "@babel"
    ],
    "rules": {
        "semi": [
            2,
            "always"
        ],
        "no-undef": 2,
        "no-unused-vars": 1
    }
}